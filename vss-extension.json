{
  "manifestVersion": 1,
  "id": null,
  "version": "2.0.1",
  "publisher": "jgarverick",
  "extensionId": "RequirementsIntegrator",
  "name": "Requirements Integrator",
  "scopes": [ "vso.work", "vso.work_write" ],
  "categories": [ "Plan and track" ],
  "description": "A requirements management and integration tool for Visual Studio Team Services.",
  "icons": {
    "default": "static/img/logo.png"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "public": true,
  "content": {
    "details": {
      "path": "overview.md"
    },
    "license": {
      "path": "LICENSE.txt"
    }
  },
  "links": {
    "home": {
      "uri": "https://github.com/jgarverick/RequirementsIntegrator"
    },
    "support": {
      "uri": "https://github.com/jgarverick/RequirementsIntegrator"
    },
    "repository": {
      "uri": "https://github.com/jgarverick/RequirementsIntegrator"
    },
    "issues": {
      "uri": "https://github.com/jgarverick/RequirementsIntegrator/issues"
    }
  },
  "branding": {
    "color": "rgb(220, 235, 252)",
    "theme": "light"
  },
  "files": [
    {
      "path": "dist",
      "addressable": true
    },
    {
      "path": "src/dist",
      "addressable": true,
      "packagePath": "lib"
    },
    {
      "path": "src/dist/cytoscape",
      "packagePath": "lib/cytoscape"
    },
    {
      "path": "node_modules/vss-web-extension-sdk/lib",
      "addressable": true,
      "packagePath": "lib"
    },
    {
      "path": "LICENSE.txt",
      "contentType": "text/plain"
    },
    {
      "path": "overview.md",
      "addressable": true,
      "contentType": "text/plain"
    },
    {
      "path": "data/SampleRequirements.xlsx",
      "addressable": true,
      "contentType": "application/vnd.ms-excel"
    },
    {
      "path": "static/img",
      "addressable": true
    },
    {
      "path": "static/css",
      "addressable": true
    },
    {
      "path": "gapAnalysis.html",
      "addressable": true
    },
    {
      "path": "importDialog.html",
      "addressable": true
    },
    {
      "path": "index.html",
      "addressable": true
    },
    {
      "path": "mappingDialog.html",
      "addressable": true
    },
    {
      "path": "sprintView.html",
      "addressable": true
    },
    {
      "path": "visualizerDialog.html",
      "addressable": true
    },
    {
      "path": "ThirdPartyNotice.txt",
      "addressable": true
    }
  ],
  "contributions": [
    {
      "id": "home",
      "targets": [
        "ms.vss-work-web.work-hub-group"
      ],
      "type": "ms.vss-web.hub",
      "properties": {
        "name": "Requirements",
        "order": 100,
        "uri": "index.html"
      }
    },
    {
      "id": "mappingDialog",
      "type": "ms.vss-web.control",
      "targets": [ ".controls" ],
      "properties": {
        "uri": "mappingDialog.html"
      }
    },
    {
      "id": "importDialog",
      "type": "ms.vss-web.control",
      "targets": [ ".controls" ],
      "properties": {
        "uri": "importDialog.html"
      }
    },
    {
      "id": "visualizerDialog",
      "type": "ms.vss-web.control",
      "targets": [ ".controls" ],
      "properties": {
        "uri": "visualizerDialog.html"
      }
    }
  ]
}